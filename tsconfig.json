{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "baseUrl": "./src",
    "paths": {
      "@components/*": ["components/*"],
      "@screens/*": ["screens/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["utils/*"],
      "@constants/*": ["constants/*"],
      "@navigations/*": ["navigations/*"],
      "@services/*": ["services/*"],
      "@types/*": ["types/*"],
      "@models/*": ["models/*"]
    }
  },
}
